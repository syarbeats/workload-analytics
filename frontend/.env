# API Configuration
VITE_API_URL=http://localhost:3000
VITE_API_TIMEOUT=10000

# Authentication
VITE_AUTH_TOKEN_KEY=workload_analytics_token
VITE_AUTH_USER_KEY=workload_analytics_user

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true

# Chart Configuration
VITE_CHART_REFRESH_INTERVAL=300000

# Development Tools
VITE_ENABLE_DEVTOOLS=true
VITE_ENABLE_MOCK_API=false

# Error Reporting
VITE_ERROR_REPORTING_URL=
VITE_ERROR_REPORTING_KEY=

# Date Format
VITE_DEFAULT_DATE_FORMAT=yyyy-MM-dd
VITE_DEFAULT_TIME_FORMAT=HH:mm:ss

# Theme
VITE_DEFAULT_THEME=light
